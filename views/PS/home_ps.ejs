<%- include("header_ps") %>
<h1>Home</h1>
<p style="font-size:1.6rem;"><%= homeContent %></p>
<% newPost.forEach(function(element) { %>
    <h1><%= element.title %></h1>
    <p style="font-size:1.6rem ;">
        <%= element.content.substring(0,100) + "...." %>
        <a href="/posts/<%=element.title %>">Continue</a>
    </p>
<% }) %>

